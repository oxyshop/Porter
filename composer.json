{
  "name": "scriptfusion/porter",
  "description": "Data import abstraction library.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "license": "LGPL-3.0",
  "require": {
    "php": "^7|^8",
    "connectors/http": "dev-oxy",
    "connectors/soap": "dev-oxy",
    "scriptfusion/static-class": "^1",
    "scriptfusion/retry": "dev-oxy",
    "scriptfusion/retry-exception-handlers": "dev-master",
    "eloquent/enumeration": "^5",
    "psr/cache": "^1|^2"
  },
  "suggest" : {
    "transformers/mapping": "Transforms array collections using Mappings."
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\": "test"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:oxyshop/HttpConnector.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:oxyshop/SoapConnector.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:oxyshop/Retry.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:oxyshop/Retry-exception-handlers.git"
    }
  ],
  "scripts": {
    "test": "phpunit -c test"
  }
}
